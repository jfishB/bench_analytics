[run]
source = .
omit =
    */tests.py
    */test_*.py
    */migrations/*
    */admin.py
    */__pycache__/*
    */venv/*
    */.venv/*
    */env/*

[report]
exclude_lines =
    pragma: no cover
    def __repr__
    def __str__
    raise AssertionError
    raise NotImplementedError
    if __name__ == .__main__.:
    if TYPE_CHECKING:
    @abstractmethod

[html]
directory = htmlcov
